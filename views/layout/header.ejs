<header class="header">
    <div class="header__container _container">
        <div class="header__body">
        <% if(header.logo) { %>
            <a href="" class="header__link-logo"><%= header.logo %></a>
        <% } %>
        <div class="header__menu menu">
            <nav class="menu__body">
                <ul class="menu__list">
                    <% header.menu_list.forEach(function(item) { %>
                    <li class="menu__item">
                        <a href="" class="menu__link"><%=item%></a>
                    </li>
                    <% }) %>
                </ul>
            </nav>
        </div>
        <div class="header__actions actions-header">
            <% header.button_list.forEach(function(item) { %>
                <a href="" class="<%= item.class %>"><%= item.label %></a>
            <% }) %>
            <button class="actions-header__burger">
                <span></span>
            </button>
        </div>
        </div>
    </div>
</header>